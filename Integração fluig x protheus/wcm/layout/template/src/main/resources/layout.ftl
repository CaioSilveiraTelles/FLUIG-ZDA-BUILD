    <section class="probootstrap-cover overflow-hidden relative"  style="background-image: url('/visao_cliente/resources/css/assets/images/background2.jpg');" data-stellar-background-ratio="0.5"  id="section-home">
      <div class="overlay"></div>
      <div class="container">
        <div class="row align-items-center">
          <div class="col-md">
            <h2 id="messageRet" class="heading mb-2 display-4 font-light probootstrap-animate">Participe, entre em contato conosco</h2>
            <p class="lead mb-5 probootstrap-animate">
              
            </p>
              <a id="btnSaiba" href="#" role="button" class="btn btn-primary p-3 mr-3 pl-5 pr-5 text-uppercase d-lg-inline d-md-inline d-sm-block d-block mb-3">Saiba mais</a> 
            </p>
          </div> 
          <div class="col-md probootstrap-animate">
            <form action="#" class="probootstrap-form">
                 <!-- CAMPO NOME -->
                <div class="form-group">
                  <div class="row mb-3">
                     <div class="col-md">
                       <div class="form-group">
                          <label for="fname" class="">nome</label>
                          <input type="text" class="form-control" id="fname" name="fname" placeholder="Insira seu nome">
                       </div>
                    </div>
                  </div>
                </div>
                <!-- CAMPO EMAIL -->
                <div class="form-group">
                  <div class="row mb-3">
                     <div class="col-md">
                       <div class="form-group">
                          <label for="fmail" class="">Email</label>
                          <input type="text" class="form-control" id="fmail" name="fmail" placeholder="email@dominio.com">
                       </div>
                    </div>
                  </div>
                </div>
              <!-- CAMPO SEXO -->
              <div class="form-group">
                <div class="row mb-3">
                  <div class="col-md">
                    <div class="form-group">
                      <label for="id_label_single">Sexo</label>

                      <label for="sexo" style="width: 100%;">
                        <input type="text" name="sexo" class="form-control" id="sexo" style="width: 100%;"/>
                      </label>
                    </div>
                  </div>
                     <!-- CAMPO ESTADO -->
                  <div class="col-md">
                    <div class="form-group">
                      <label for="id_label_single2">Estado</label>
                      <div class="">
                          <label for="estado" style="width: 100%;">
                       
                        <input type="text" class="form-control" id="estado" name="estado" style="width: 100%;" />
                           
                      </label>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
               <!-- CAMPO PERGUNTA -->
                 <div class="row mb-3">
                  <div class="col-md">
                    <div class="form-group">
                      <label for="pergunta">Cliente fluig :</label>
                        <input type="text" class="form-control"class="js-example-basic-single js-states form-control" id="pergunta" name="pergunta" style="width: 100%;">
                 
                    </div>
                  </div>
                </div>
               
                 <!-- CAMPO MENSAGEM -->
                <div class="row mb-5">
                  <div class="col-md">
                    <div class="form-group">
                      <label for="probootstrap-date-departure">Mensagem</label>
                        <textarea id="message" name="message" class="form-control"> </textarea>       
                    </div>
                  </div>
                </div>
                   <!-- BOTÃƒO ENVIAR -->
                <div class="row">
                  <div class="col-md">
                    <input type="button" value="Enviar" id="enviar_contato" class="btn btn-primary btn-block">
                  </div>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
